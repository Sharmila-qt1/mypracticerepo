pipeline {
    agent {label 'MAVEN3 && NVM && PYTHON3'} 
    triggers{
        pollSCM(* * * * *)
    }
    stages {
        stage('clone') { 
            steps {
                git url:'https://github.com/Sharmila-qt1/shopizer.git', branch : 'SPRINT_1_REL' 
            }
        }
        stage('install') { 
            steps {
               sh 'mvn package'
            }
        }
    }
}