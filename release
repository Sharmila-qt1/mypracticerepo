pipeline {
    agent {label 'MAVEN3 && NVM && PYTHON3'} 
    triggers{
        cron('30 17 * * *')
    }
    stages {
        stage('clone') { 
            steps {
                git url:'https://github.com/Sharmila-qt1/shopizer.git', branch : 'develop' 
            }
        }
        stage('install') { 
            steps {
               sh 'mvn package'
            }
        }
    }
}